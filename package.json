{
  "name": "react-boilerplate",
  "version": "3.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/mxstbr/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=3"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run test && npm run build:clean",
    "build:clean": "rimraf ./build/*",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "analyze": "node ./internals/scripts/analyze.js",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "pagespeed": "node ./internals/scripts/pagespeed.js",
    "presetup": "npm i chalk",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:css": "stylelint ./app/**/*.css",
    "lint:staged": "lint-staged",
    "pretest": "npm run lint",
    "precommit": "npm test",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:debug": "npm run test -- --debug --no-single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "lint-staged": {
    "eslint": "*.js",
    "stylelint": "*.css"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "max-len": 0,
      "react/jsx-filename-extension": 0,
      "import/no-extraneous-dependencies": 0,
      "react/forbid-prop-types": 0,
      "react/no-unused-prop-types": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.test.babel.js"
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "c3": "0.4.11",
    "chalk": "1.1.3",
    "chance": "1.0.6",
    "compression": "1.6.2",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "express": "4.15.2",
    "faker": "4.1.0",
    "file-loader": "0.10.1",
    "fontfaceobserver": "2.0.9",
    "highcharts": "5.0.9",
    "history": "3.3.0",
    "immutable": "3.8.1",
    "material-ui": "0.17.1",
    "moment": "2.18.0",
    "node-github": "0.0.3",
    "offline-plugin": "4.6.2",
    "react": "15.4.2",
    "react-addons-shallow-compare": "15.4.2",
    "react-c3-component": "1.3.3",
    "react-data-grid": "2.0.18",
    "react-dom": "15.4.2",
    "react-highcharts": "11.5.0",
    "react-nvd3": "0.5.7",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.5",
    "react-router-scroll": "0.2.0",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.14.3",
    "reselect": "2.5.4",
    "sanitize.css": "5.0.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-cli": "6.24.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.4.1",
    "babel-plugin-istanbul": "4.0.0",
    "babel-plugin-react-intl": "2.3.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-es2015-webpack": "6.4.3",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.22.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "cheerio": "0.22.0",
    "coveralls": "2.11.16",
    "cross-env": "3.2.4",
    "enzyme": "2.7.1",
    "css-loader": "0.27.3",
    "enzyme": "2.7.1",
    "eslint": "3.18.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.0",
    "eslint-plugin-react": "6.10.3",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "expect-jsx": "3.0.0",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.9.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "3.2.0",
    "imports-loader": "0.7.1",
    "ip": "1.1.5",
    "istanbul": "0.4.5",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.2.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "lint-staged": "3.4.0",
    "minimist": "1.2.0",
    "mocha": "3.1.2",
    "ngrok": "2.2.6",
    "node-inspector": "0.12.8",
    "null-loader": "0.1.1",
    "plop": "1.7.4",
    "postcss-cssnext": "2.10.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "1.3.3",
    "postcss-reporter": "2.0.0",
    "pre-commit": "1.2.2",
    "psi": "3.0.0",
    "rimraf": "2.6.1",
    "shelljs": "0.7.7",
    "style-loader": "0.14.0",
    "sinon": "2.0.0",
    "style-loader": "0.14.1",
    "stylelint": "7.9.0",
    "stylelint-config-standard": "16.0.0",
    "url-loader": "0.5.8",
    "webpack-dev-middleware": "1.10.1",
    "webpack": "2.1.0-beta.22",
    "webpack-hot-middleware": "2.17.1"
  }
}
